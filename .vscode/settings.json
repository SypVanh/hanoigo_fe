{
  "dart.lineLength": 120,
  "[dart]": {
    "editor.rulers": [120]
  },
  "search.exclude": {
    "**/generated": true
  },
  "dart.debugSdkLibraries": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "eslint.enable": true,
  "eslint.validate": ["javascript"]
}
